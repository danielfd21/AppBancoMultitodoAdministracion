<Window x:Class="AppBancoMultitodoAdministracion.Vista.VistaContabilidad.NuevaClaveCuenta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppBancoMultitodoAdministracion.Vista.VistaContabilidad"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="NuevaClaveCuenta" Height="450" Width="800">
    <Grid>
        <Label   Content="Cuenta:"  HorizontalAlignment="Left" Margin="173,107,0,0" VerticalAlignment="Top" Width="137"/>
        <TextBox    Name="txt_cuenta"  HorizontalAlignment="Left" Margin="291,104,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="192" Height="31"  />
        <Label   Content="Clave:"  HorizontalAlignment="Left" Margin="173,186,0,0" VerticalAlignment="Top" Width="137"/>
        <TextBox   Name="txt_clave_cuenta"  HorizontalAlignment="Left" Margin="291,181,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="192" Height="31" IsEnabled="false" />
        
        <Button  Content="Generar Clave" HorizontalAlignment="Left" Margin="291,253,0,0" VerticalAlignment="Top" Height="34" Width="188" Click="Generar_Clave"/>

    </Grid>
</Window>
